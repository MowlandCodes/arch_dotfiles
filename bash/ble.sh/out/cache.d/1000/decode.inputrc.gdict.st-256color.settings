bind -m emacs '"\e\e[D": backward-word'
bind -m emacs '"\e[5D": backward-word'
bind -m emacs '"\e[5~": beginning-of-history'
bind -m emacs '"\e[7~": beginning-of-line'
bind -m emacs '"\e\e\C-@": complete'
bind -m emacs '"\e[6~": end-of-history'
bind -m emacs '"\e[8~": end-of-line'
bind -m emacs '"\e\e[C": forward-word'
bind -m emacs '"\e[5C": forward-word'
bind -m emacs '"\e[2~": quoted-insert'
bind -m emacs '"\e[0n": redraw-current-line'
